{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".js$"
  },
  "plugins": ["node_modules/jsdoc/plugins/markdown"],
  "markdown": {
    "tags": ["param", "deprecated", "description"]
  },
  "opts": {
    "template": "node_modules/docdash/",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
